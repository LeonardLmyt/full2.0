/**app.wxss**/
page {
	background-color: #fff;
  word-break: break-all;
}
.navigator-hover{background-color: initial !important;opacity: 1 !important;}
.list-item{
  padding: 24rpx 20rpx;
  border-bottom: 2rpx solid #f0f0f0;
  background: #fff;
}
.item-detail{
  width: 100%;
}
.flex-inline{
  display: flex;
}
.flex-inline .item-detail{
  width: 68%;
  margin-right: 2%;  
}
.flex-inline .li-title{
  height: 88rpx;
}
.li-title{
  font-size: 36rpx;
  line-height: 44rpx;
  font-weight: 400;
  color: #222;
  -webkit-line-clamp: 2;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
}
.li-img{
  margin-top: 24rpx;
  width: 100%;
  overflow: hidden;
}
.li-img__holder{
  position:relative;
  font-size: 0;
  width: 31.3%;
  margin-right: 2%;
  float: left;
}
.li-img__holder:nth-child(3){
  margin-right: 0;
}
.li-imgratio--rect{
  padding-top: 60%;
  position: relative;
  width: 100%;
}
.li-imgratio--rect image{
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  object-fit: cover;
}
.li-info{
  margin-top: 20rpx;
  color: #bccbd5;
  font-size: 24rpx;
}
.icon-user{
  background: url(data:image/png;base64,) no-repeat 0 50%;
  background-size: 24rpx auto;
  padding-left: 30rpx;
  display: inline;
}
.icon-views{
  background: url(data:image/png;base64,) no-repeat 0 50%;
  background-size: 28rpx auto;
  padding-left: 34rpx;
  margin-left: 20rpx;
  display: inline;
}
.button-primary{
  background-color: #6A9CF4;
  border-radius: 8rpx;
  border: 0 none;
  color: #fff;
  font-size: 32rpx;
  height: 84rpx;
  line-height: 84rpx;
  width: 100%;
  text-align: center
}
.ipt{
  border: 2rpx solid #ccc;
  height: 84rpx;
  font-size: 30rpx;
  color: #333;
  padding: 0 24rpx;
  border-radius: 8rpx;
}

.footer{position: fixed;bottom: 0;left: 0;right: 0;height: 100rpx;box-shadow: 0 0 10px #ddd;-webkit-box-shadow: 0 0 10px #ddd;}
.footer_sub{display: -webkit-box;display:box;width: 100%;height: 100%;overflow: hidden;background-color: #ffffff;}
.footer_tab{display: -webkit-box;display:box;-webkit-box-flex: 1.0;box-flex: 1.0;-webkit-box-pack: center;box-pack: center;width: 1%;-webkit-box-orient: vertical;box-orient: vertical;-webkit-box-align: center;box-align: center;}
.footer_tab view:first-child{width: 50rpx;height: 50rpx;background-size: 100% 100%;-webkit-background-size: 100% 100%;}
.footer_tab:nth-child(1) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab:nth-child(2) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab:nth-child(3) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab:nth-child(4) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab view:last-child{font-size: 22rpx;color: #999999;}


/*layer*/
.layer{position: fixed;top: 40%;left: 0;right: 0;width: 100%;overflow: hidden;pointer-events: none;z-index: 9999;}
.layer-sub{display:-webkit-box;display:box;width: 100%;overflow: hidden;}
.layer-sub view{-webkit-box-flex: 1.0;box-flex: 1.0;}
.layer-content{-webkit-box-flex-group: 2;flex-group: 2;background-color: rgba(0,0,0,.6);color: #fff;padding: 20rpx 30rpx;border-radius: 10rpx;-webkit-border-radius: 10rpx;height: 40rpx;line-height: 40rpx;border: 1px solid #000;}
.layer-warning{animation: layerErrorAnimation 1s;-webkit-animation: layerErrorAnimation 1s;color: #8a6d3b;background-color: #fcf8e3;border: 1px solid #faebcc;}
.layer-error{animation: layerErrorAnimation 1s;-webkit-animation: layerErrorAnimation 1s;color: #a94442;background-color: #f2dede;border: 1px solid #ebccd1;}
 @keyframes layerErrorAnimation {0%, 100% {-webkit-transform: translateX(0);}10%,30%,50%,70%,90% {-webkit-transform: translateX(-5px);}20%,40%,60%,80% {-webkit-transform: translateX(5px);}} 
@-webkit-keyframes layerErrorAnimation {0%, 100% {-webkit-transform: translateX(0);}10%,30%,50%,70%,90% {-webkit-transform: translateX(-5px);}20%,40%,60%,80% {-webkit-transform: translateX(5px);}}