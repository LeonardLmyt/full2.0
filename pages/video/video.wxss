.content_sub{width: 92%;overflow: hidden;margin: 0 auto;}
.section{position: relative;width: 100%;overflow: hidden;margin: 47rpx 0 30rpx;}
.content_sub:first-child .section{margin-top: 30rpx;}
.section image{width: 100%;height: 350rpx;}
.text_contens{display: -webkit-box;display:box;-webkit-box-align: center;box-align: center;width: 100%;overflow: hidden;font-size: 24rpx;color: #999999;line-height:40rpx;}
.text_contens view{width:3px;height:3px;border-radius:50%;-webkit-border-radius:50%;margin:0 15rpx;
background-color:#999;line-height: 40rpx;}
.file{position: absolute;left: -webkit-calc(50% - 48rpx);top: -webkit-calc(50% - 48rpx);left: calc(50% - 48rpx);top: calc(50% - 48rpx);width: 96rpx;height: 96rpx;background: url(data:image/png;base64,);background-size: 100% 100%;-webkit-background-size: 100% 100%;}
.title_main{display: -webkit-box;display:box;position: absolute;top: 0;left: 20rpx;width: calc(100% - 40rpx);width: -webkit-calc(100% - 40rpx);height: calc(100% - 25rpx);height: -webkit-calc(100% - 25rpx);-webkit-box-pack: start;box-pack:start;-webkit-box-orient: vertical;box-orient: vertical;color: #fff;text-shadow: 1rpx 1rpx 1rpx #000;}
.title_main view{display: -webkit-box;display:box;width: 100%;font-size: 34rpx;line-height: 46rpx;max-height:90rpx;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;display:box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-break:break-word;}

.post_tow{width: 40px;height: 40px;position: fixed;right: 4%;bottom: 20%;opacity: 0;transition: all 0.2s;-webkit-transition: all 0.2s;z-index: 100;}
.post_tow image{width: 40px;height: 40px;}
.post_tow_animation{animation:post_tow 5s;-webkit-animation:post_tow 5s;}
.post_tow_animation1{animation:post_tow1 5s;-webkit-animation:post_tow1 5s;}
@keyframes post_tow{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@-webkit-keyframes post_tow{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@keyframes post_tow1{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%
{opacity:0.2;}100% {opacity:0;}}
@-webkit-keyframes post_tow1{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}


.footer_tab:nth-child(3) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab:nth-child(3) view:last-child{color:#4E74D8;}
