.content{width:100%;overflow: hidden;background:white;margin-top:20rpx; }
.midden_lisy{position: relative;width: 100%;overflow: hidden;border-bottom: 2rpx solid #f0f0f0;}
.midden_lisy_sub{box-sizing: border-box;-webkit-box-sizing: border-box;width: 100%;overflow: hidden;}
.Written_content{line-height:45rpx;max-height:90rpx;overflow:hidden;text-overflow:ellipsis;display:-webkit-box;display:box;-webkit-line-clamp:2;-webkit-box-orient:vertical;word-break:break-word;font-size:36rpx;color:#222;margin: 20rpx 20rpx 0 20rpx;}
.Written_img image{height: 100%;width: 100%;}
.Written_img{margin: 4% auto 0;height: 300rpx;width: 92%;}
.all_count{width: auto;display: -webkit-box;display: box;}
.li-info{
margin-bottom: 24rpx;
  color: #bccbd5;
  font-size: 24rpx;
}
.icon-user{background: url(data:image/png;base64,) no-repeat 0 50%;
  background-size: 24rpx auto;
  padding-left: 30rpx;
  display: inline;
}
.icon-views{
  background: url(data:image/png;base64,) no-repeat 0 50%;
  background-size: 28rpx auto;
  padding-left: 34rpx;
  margin-left: 20rpx;
  display: inline;
}

.li-infox{width: 92%;overflow: hidden;margin: 0 4%;}
.browse{width: 100%;display: -webkit-box;display: box;-webkit-box-align:center;box-align:center;}
.browse>view{display: -webkit-box;display: box;-webkit-box-flex: 1.0;box-flex: 1.0;font-size: 24rpx;color: #999999;line-height: 61rpx;} 
.icon_name{
font-size: 28rpx;
color: #333333;
-webkit-box-flex-group: 2;
box-flex-group: 2;
 margin-right: 80rpx;
 line-height: inherit !important;
}
.up{position: relative;width: 60rpx;height: 60rpx;padding: 20rpx;border: 2rpx solid #FB3228;border-radius: 50% 50%;-webkit-border-radius: 50% 50%;}
.midden_lisy_absolute_show{transform: translateX(0);-webkit-transform: translateX(0);}


.my_last{margin-top:206rpx;display: -webkit-box;display:box;-webkit-box-pack: center;box-pack: center;}
.my_last image{width: 340rpx;height: 340rpx;}
.nocontent{font-size: 32rpx;color: #999;text-align: center;}