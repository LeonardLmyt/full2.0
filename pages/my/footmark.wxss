page{background-color:#F4F4F4;}
.title_name{width: 100%;background-color: #f4f4f4;font-size: 25rpx;display: -webkit-box;display: box;padding: 10rpx;color: #333333;
}

/*two*/
.two{display: -webkit-box;display:box;box-sizing:border-box;
-webkit-box-sizing:border-box;
width:100%;
overflow:hidden;
background-color:#fff;
margin-top:20rpx;
padding:15rpx 3%;
}
.two_main{width: 100%;overflow: hidden;border-top: 10rpx solid #f2f2f2;}
.two_main .two:first-child{margin-top: 0}
.two_left{width: auto;overflow: hidden;}
.two_left image{width: 180rpx;margin-left: 20rpx;}
.two_right{-webkit-box-flex:1.0;box-flex:1.0;}
.two_title{line-height:1.1em;
height:2.1em;
overflow:hidden;
text-overflow:ellipsis;
display:-webkit-box;
display:box;
-webkit-line-clamp:2;
-webkit-box-orient:vertical;
word-break:break-word;
font-size: 32rpx;
color: #333333;
}






.name_pople image{width: 35rpx;
height: 35rpx;display:inline-block;
vertical-align:middle;
margin-right: 10rpx;
}
.name_how image{width: 35rpx;
height: 35rpx;display:inline-block;
vertical-align:middle;
margin-right: 10rpx;
}
.name_pople {color: #999999;font-size: 24rpx;-webkit-box-flex: 1.0;box-flex: 1.0;text-align: right}
.name_how {color: #999999;font-size: 24rpx;-webkit-box-flex: 1.0;box-flex: 1.0;text-align: right;}
.big_list{display: -webkit-box;display: box;font-size: 24rpx;color: #999;margin-top: 43rpx;}
