.titie_img {
  width: 100%;
  height: 360rpx;
  position: relative;
}

.img_bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 360rpx;
}

.set {
  display: block;
  z-index: 100;
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  background-color: #ededed;
  color: #333;
  font-size: 24rpx;
  padding: 3rpx 20rpx;
  border-radius:20rpx;
  -webkit-border-radius:20rpx;
}

.img_bg image {
  width: 100%;
  height: 360rpx;
}

.binding {
  width: 144rpx;
  height: 48rpx;
  overflow: hidden;
  background-color: #548ffb;
  margin: 16rpx auto 0;
  padding: 0;
  line-height: 48rpx;
  color: #fff;
  font-size: 24rpx;
  border-radius: 50rpx;
  -webkit-border-radius: 50rpx;
  box-shadow: 1rpx 1rpx 16rpx rgba(84, 143, 251, 0.6);
  -webkit-box-shadow: 1rpx 1rpx 16rpx rgba(84, 143, 251, 0.6);
}

.head_portrait {
  width: 168rpx;
  height: 168rpx;
  overflow: hidden;
  background: white;
  margin: 21rpx auto 5rpx;
  border-radius: 50% 50%;
  -webkit-border-radius: 50% 50%;
}

.head_portrait image {
  box-sizing: border-box;
  -webkit-box-sizing: border-box;
  width: 100%;
  height: 100%;
  border-radius: 50% 50%;
  -webkit-border-radius: 50% 50%;
  border: 1px solid #ddd;
}

.all_content {
  width: 100%;
  height: auto;
  background: white;
}

.text_name {
  font-size: 32rpx;
  color: #333;
}

.hader_all {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 360rpx;
  text-align: center;
  overflow: hidden;
  z-index: 100;
}

.big_listcontent {
  height: 180rpx;
  width: 100%;
  margin-top: 123rpx;
}

.all_list {
  display: -webkit-box;
  display: box;
  -webkit-box-align: center;
  width: 90%;
  height: 100%;
  text-align: center;
  margin: 0 auto;
}

.all_list>view {
  margin-left: 20rpx;
  height: 150rpx;
  display: -webkit-box;
  display: box;
  -webkit-box-flex: 1.0;
  box-flex: 1.0;
  -webkit-box-align: center;
  -webkit-box-pack: center;
  width: 1%;
  /* border-left: 1px solid #ccc; */
}

.all_list view:nth-child(1) {
  margin-left: 0;
  border-left: 0;
}

.all_list_sub {
  width: 100%;
  height: 100%;
}

.all_list_sub image {
  width: 128rpx;
  height: 128rpx;
}

.all_list_sub view {
  color: #666;
  font-size: 30rpx;
}

