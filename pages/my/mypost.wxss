
/*content*/
.content{width: 100%;overflow: hidden;margin-top: 64rpx;}
.content_sub{display: -webkit-box;display:box;width: 100%;overflow: hidden;}
.content_sub_left{width: 16%;-webkit-box-pack: center;-webkit-box-align: center;text-align:-webkit-center;border-right: 1px solid #F4F4F4;}
.time{text-align: center;color:#353535;font-size: 26rpx;margin:10rpx 0;}
.content_sub_right{position: relative;display: -webkit-box;display:box;-webkit-box-flex: 1.0;box-flex: 1.0;padding: 0 2% 82rpx 5%;-webkit-box-orient: vertical;box-orient: vertical;}
.content_title{color:#333333;font-size:32rpx;width: 100%;overflow: hidden;text-overflow: ellipsis;display: -webkit-box;display: box;-webkit-box-orient: vertical;word-break: break-word;}
.content_sub_right_img{width: 100%;overflow:hidden;margin: 12rpx 0 12rpx 0;}
.content_sub_right_img image{width: 300rpx;height: 200rpx;}
.content_sub_right_buttom{display: -webkit-box;display:box;-webkit-box-align: center;box-align: center;color: #999;font-size: 24rpx;line-height: 40rpx;}
.content_sub_right_buttom view{width:3px;
height:3px;
border-radius:50%;
-webkit-border-radius:50%;
margin:0 15rpx;
background-color:#999;
}
.content_sub_icon{position: absolute;top: 7rpx;left: -14rpx;width: 30rpx;height: 30rpx;background-image: url(data:image/png;base64,);background-size: 100% 100%;-webkit-background-size: 100% 100%;}

.my_last{margin-top:206rpx;display: -webkit-box;display:box;-webkit-box-pack: center;box-pack: center;}
.my_last image{width: 340rpx;height: 340rpx;}
.nocontent{font-size: 32rpx;color: #999;text-align: center;}