.navbar-fixed{
  width: 750rpx;
  height: 110rpx;
  box-shadow: 0 20rpx 0 0 #f7f8fd;
  background-color: #fff;
  z-index: 999;
  display: flex;
  position: fixed;
  padding: 0 80rpx 0 20rpx;
  top: 0;
  box-sizing: border-box; 
}
.slider-wrap{
  white-space: nowrap;
}
.item-cell{
  display: inline-block;
  padding: 21rpx 33rpx;
  font-size: 30rpx;
  color: #333333;
  line-height: 67rpx;
}
.selected{
  color: #4988fb;
  border-bottom: 2rpx solid #4988fb;
}
.category-view{
  position: absolute;
  top:15rpx;
  right: 17rpx;
  display: flex;
}
.category-dotline{
  width: 16rpx;
  height: 86rpx;
  background: url(data:image/png;base64,) no-repeat top left;
  background-size: auto 86rpx;
}
.category-icon image{
  width: 83rpx;
  height: 83rpx;


}

.swiper{
  display: block;
  width: 100%;
  overflow: hidden;
  margin: 0;
  border: 0;
  margin-top: 120rpx;
  background-color: #fff;
}

.scrollView{
  height: 100%;
  overflow: hidden;
}



