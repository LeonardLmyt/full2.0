.navbar-fixed{
  width: 750rpx;
  height: 86rpx;
  box-shadow: 0 2rpx 0 0 #eee;
  background-color: #f6f7f8;
  z-index: 999;
  display: flex;
  position: fixed;
  padding: 0 80rpx 0 20rpx;
  top: 0;
  box-sizing: border-box; 
}
.slider-wrap{
  white-space: nowrap;
}
.item-cell{
  display: inline-block;
  padding: 21rpx 27rpx;
  font-size: 28rpx;
  color: #666;
  line-height: 40rpx;
}
.selected{
  color: #4988fb;
  border-bottom: 8rpx solid #4988fb;
}
.category-view{
  position: absolute;
  right: 0;
  display: flex;
}
.category-dotline{
  width: 16rpx;
  height: 86rpx;
  background: url(data:image/png;base64,) no-repeat top left;
  background-size: auto 86rpx;
}
.category-icon{
  width: 80rpx;
  height: 86rpx;
  background: url(data:image/png;base64,) no-repeat 50% 28rpx;
  background-size: 24rpx auto;
  box-shadow: -2px -2px 8px #999999;
  -webkit-box-shadow: -2px -2px 8px #999999;
}

.swiper{
  display: block;
  width: 100%;
  overflow: hidden;
  margin: 0;
  border: 0;
  margin-top: 86rpx;
  background-color: #fff;
}

.scrollView{
  height: 100%;
  overflow: hidden;
}
.midden_lisy{height: auto;width: 100%;overflow: hidden;border-bottom: 2rpx solid #f0f0f0;}
.middens_lisy{height: auto;width: 100%;overflow: hidden;border-bottom: 2rpx solid #f0f0f0;}
.Written_content{ line-height:45rpx;
max-height:90rpx;
overflow:hidden;
text-overflow:ellipsis;
display:-webkit-box;
display:box;
-webkit-line-clamp:2;
-webkit-box-orient:vertical;
word-break:break-word;
font-size:36rpx;
color:#222;
margin: 20rpx 20rpx 0 20rpx;
}
.Written_img image{height: 100%;width: 100%;}
.Written_img{margin: 20rpx auto 0;height: 300rpx;width: 92%;}

.mie{
  padding: 20rpx 0;
  display: inline-block;
}
.li-infox{width: 92%;overflow: hidden;margin: 0 4%;}
.all_count{width: auto;display: -webkit-box;display: box;}
.icon-userx{
  display:inline-block;
font-size: 28rpx;
color: white;
text-align: center;
border-radius:40rpx;
background: #8CD91E;
padding: 6rpx 4%;
width: auto;
}
.icon-u{
  display:inline-block;
font-size: 28rpx;
color: white;
text-align: center;
border-radius:40rpx;
background: #FA752B;
padding: 6rpx 4%;
width: auto;
}
.icon-us{
  display:inline-block;
font-size: 28rpx;
color: white;
text-align: center;
border-radius:40rpx;
background: #207FD9;
padding: 6rpx 4%;
width: auto;
}
.li-info{
margin-bottom: 24rpx;
  color: #bccbd5;
  font-size: 24rpx;
}
.icon-user{
  background: url(data:image/png;base64,) no-repeat 0 50%;
  background-size: 24rpx auto;
  padding-left: 30rpx;
  display: inline;
}
.icon-views{
  background: url(data:image/png;base64,) no-repeat 0 50%;
  background-size: 28rpx auto;
  padding-left: 34rpx;
  margin-left: 20rpx;
  display: inline;
}

.icon_content{
display:inline-block;
font-size: 28rpx;
margin-left: 2%;
color: #333333;
}

.icon_browse{
font-size: 26rpx;
text-align: right;
color: #666666; 
}
.icon_time{
font-size: 26rpx;
width: auto;
text-align: right;
color: #666666;
}

.browse{width: 100%;display: -webkit-box;display: box;-webkit-box-align:center;box-align:center;}
.browse>view{display: -webkit-box;display: box;-webkit-box-flex: 1.0;box-flex: 1.0;font-size: 24rpx;color: #999999;line-height: 61rpx;} 
.icon_name{
font-size: 28rpx;
color: #333333;
-webkit-box-flex-group: 2;
box-flex-group: 2;
 margin-right: 80rpx;  
 line-height: inherit !important;
}


.post_tow{width: 40px;height: 40px;position: fixed;right: 4%;bottom: 20%;opacity: 0;transition: all 0.2s;-webkit-transition: all 0.2s;z-index: 100;}
.post_tow image{width: 40px;height: 40px;}
.post_tow_animation{animation:post_tow 5s;-webkit-animation:post_tow 5s;}
.post_tow_animation1{animation:post_tow1 5s;-webkit-animation:post_tow1 5s;}
@keyframes post_tow{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@-webkit-keyframes post_tow{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@keyframes post_tow1{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%
{opacity:0.2;}100% {opacity:0;}}
@-webkit-keyframes post_tow1{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}

