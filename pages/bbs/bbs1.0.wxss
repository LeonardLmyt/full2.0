
.scrollView{width:100%;}
.navbar-fixed{
  width: 750rpx;
  height: 86rpx;
  box-shadow: 0 2rpx 0 0 #eee;
  background-color: #f6f7f8;
  z-index: 999;
  display: flex;
  position: fixed;
  padding: 0 80rpx 0 20rpx;
  top: 0;
  box-sizing: border-box
}
.slider-wrap{
  white-space: nowrap;
}


.category-view{
  position: absolute;
  right: 0;
  display: flex;
}
.category-dotline{
  width: 16rpx;
  height: 86rpx;
  background: url(data:image/png;base64,) no-repeat top left;
  background-size: auto 86rpx;
}

.swiper{
  display: block;
  width: 100%;
  overflow: hidden;
  margin: 0;
  border: 0;
  background-color: #fff;
}
.scrollView{
  height: 20%;
}


/*  加载更多   */
.weui-loadmore {
  width: 65%;
  margin: 35rpx auto;
  line-height: 40rpx;
  font-size: 14px;
  text-align: center;
}
.weui-loadmore__tips {
  display: inline-block;
  vertical-align: middle;
}
.weui-loading {
  margin: 0 5px;
  width: 20px;
  height: 20px;
  display: inline-block;
  vertical-align: middle;
  -webkit-animation: weuiLoading 1s steps(12, end) infinite;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
}

.post_tow{width: 40px;height: 40px;position: fixed;right: 4%;bottom: 20%;opacity: 0;transition: all 0.2s;-webkit-transition: all 0.2s;z-index: 100;}
.post_tow image{width: 40px;height: 40px;}
.post_tow_animation{animation:post_tow 5s;-webkit-animation:post_tow 5s;}
.post_tow_animation1{animation:post_tow1 5s;-webkit-animation:post_tow1 5s;}
@keyframes post_tow{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@-webkit-keyframes post_tow{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@keyframes post_tow1{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}
@-webkit-keyframes post_tow1{0%{opacity:0.9;}20%{opacity:0.9;}40%{opacity:0.9;}60%{opacity:0.6;}70%{opacity:0.5;}80%{opacity:0.3;}90%{opacity:0.2;}100% {opacity:0;}}

.footer_tab:nth-child(1) view:first-child{background-image:url(data:image/png;base64,)}
.footer_tab:nth-child(1) view:last-child{color:#4E74D8;}

